<section class="nav-wrapper">
  <nav>
    <div class="logoWrapper">
      <h1>MemeMe</h1>

      <img src="../images/Frame 12.png" alt="">
    </div>
    <input type="text">

    {{#unless isLoggedIn}}
    <a href="/login">LOG IN</a>
 
    <a href="/signup">SIGN UP</a>
    {{/unless}}
      
    {{#if isLoggedIn}}
    <a href="/meme/add">Add a meme</a>
    <a href="/profile">Profile {{username}}</a>
    {{/if}}
   
  </nav>
  <header>
    <img src="../images/background-meme.png" alt="">
  </header>
</section>

<section class="img-wrapper">
  <div id="imgList">
    {{#each imgList}}
      <div>
        <h1>{{this.title}}</h1>
        <a href="/meme/{{_id}}"><img src="{{this.imgPath}}" alt="{{this.title}}"> </a> 

      </div>
    {{/each}}
  </div>
  <div class="button-wrapper">
    <button class="see-more">SEE MORE</button>
  </div>
</section>
<section class="footer-wrapper">
  <footer>
    <a href="">(c)2020 MEMEME</a>
    <a href="">ABOUT</a>
    <a href="">PRIVACY</a>
    <a href="">TERMS</a>
  </footer>
</section>